@model GradeViewModel

<link href="~/css/CheckMyGradesPage.css" rel="stylesheet" />

<body>
    <table class="studentInfo">
        <tbbody>
            <tr>
                <td class="header" colspan="5">Cijferlijst</td>
            </tr>
            <tr>
                <td>Schoolvak</td>
                <td>Toetsnaam</td>
                <td>Cijfer</td>
            </tr>
            @for (int i = 0; i < Model.GradeList.Count; i++)
            {
                <tr>
                    <td>@Model.TestList[i].Subject</td>
                    <td>@Model.TestList[i].Name</td>
                    <td>@Model.GradeList[i].GradeProp</td>
                </tr>
            }
        </tbbody>
    </table>
</body>